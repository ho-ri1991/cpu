3a : 01094020; % (e8)         add  $8, $8, $9     # sum                    %
3b : 20a5ffff; % (ec)         addi $5, $5, -1     # counter - 1            %
3c : 14a0fffc; % (f0)         bne  $5, $0, loop   # finish?                %
3d : 20840004; % (f4) dslot5: addi $4, $4,  4     # address + 4, ds        %
3e : 03e00008; % (f8)         jr   $31            # return                 %
