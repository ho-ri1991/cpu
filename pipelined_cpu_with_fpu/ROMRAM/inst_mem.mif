11 : 46030840; % (44)         add.s  f1,  f1, f3  # stall 1                %
12 : 46030841; % (48)         sub.s  f1,  f1, f3  # stall 2                %
13 : e4210030; % (4c)         swc1   f1,  48($1)  # stall 1                %
14 : c4050004; % (50)         lwc1   f5,  04($0)  # load fp data           %
